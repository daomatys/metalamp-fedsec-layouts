include /components/title-bar/title-bar.pug

mixin rangeSlider( item )
  - const stateId = item.id + '_state'
  - const widthClass = `elem__width_${ item.widthbypercent }`

  .range-slider__wrap( class=widthClass )
    +titleBar( item.title, true, stateId )
    .range-slider__container
      .range-slider__elem( id=item.id )
