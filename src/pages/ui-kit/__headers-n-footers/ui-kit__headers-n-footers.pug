extends /pages/ui-kit/ui-kit.pug


block head 
  title UI KIT / Headers & Footers


block header
  include /components/logo/logo.pug

  +logo({ type: "monochrome" })


block main
  include /components/header/header.pug
  include /components/footer/footer.pug

  .headers-n-footers
    +header({ authorized: false })
    +header({ authorized: true })
    +footer({ type: 'default' })
    +footer({ type: 'mobile' })
    